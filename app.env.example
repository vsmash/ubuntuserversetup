# Non-root user for deployments, devlog, cron jobs etc.
APP_USER="ubuntu"

LOGO="logo_foo"
SERVER_HOST="your webserver name"
APP_SLACK_MARK_WEBHOOK="https://hooks.slack.com/services/xxxx"
APP_SLACK_MONITOR_WEBHOOK="https://hooks.slack.com/services/xxxx"
APP_SLACK_WEBHOOK="${APP_SLACK_MARK_WEBHOOK}"
APP_SLACK_ICON_EMOJI="logo_foo"
APP_SLACK_CHANNEL="monitoring"
APP_SLACK_USERNAME="your webserver name"
SYSOP_IP="IPFORFIREWALL"

# Devlog identity
DEVLOG_CLIENT="Your Company"
DEVLOG_SUBCLIENT="Your Project"
DEVLOG_PROJECT="VPS Devops"

export devlogging=true
export logingooglespreadsheet=true
export googleserviceaccount="/etc/serversetup/credentials/service-account.json"
export logfilegooglespreadsheetid="YOUR_SPREADSHEET_ID"
export bbb="/opt/serversetup"

# Site database config (used by import/export scripts)
SITE_DOMAIN="example.com"
LOCAL_DB_NAME="example_com"
LOCAL_DB_USER="root"
SHARED_SERVER_ALIAS="sharedserver"
SHARED_SITE_PATH="domains/example.com/public_html"